// first exercise in method and association

// creating a rectangle to be a type
struct Rectangle {
    width: u32,
    height: u32,
}

//implementation of our type Rectangle
impl Rectangle {
    fn area(&self) -> u32 {
        self.width * self.height
    }
}

fn main() {
//instantiating with rect1 variable
    let rect1 = Rectangle { width: 30, height: 50 };

    println!("Area is {}", rect1.area());
}
